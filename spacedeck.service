[Unit]
Description=Work Together Visually
Documentation=https://spacedeck.com/
After=network.target

[Service]
Environment=NODE_PORT=9666
Type=simple
User=spacedeck
WorkingDirectory=/home/spacedeck/spacedeck
ExecStart=/usr/bin/node /home/spacedeck/spacdeck/spacedeck.js
Restart=always

[Install]
WantedBy=multi-user.target
